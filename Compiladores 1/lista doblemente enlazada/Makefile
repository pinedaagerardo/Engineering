####### Compile

.obj/lista.o: lista.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/lista.o lista.cpp

.obj/main.o: main.cpp .ui/practica1.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/main.o main.cpp

.ui/practica1.h: practica1.ui 
	$(UIC) practica1.ui -o .ui/practica1.h

.ui/practica1.cpp: .ui/practica1.h practica1.ui practica1.ui.h 
	$(UIC) practica1.ui -i practica1.h -o .ui/practica1.cpp

.obj/practica1.o: .ui/practica1.cpp practica1.ui.h \
		.ui/practica1.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/practica1.o .ui/practica1.cpp

.obj/moc_practica1.o: .moc/moc_practica1.cpp  .ui/practica1.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_practica1.o .moc/moc_practica1.cpp

.moc/moc_practica1.cpp: $(MOC) .ui/practica1.h
	$(MOC) .ui/practica1.h -o .moc/moc_practica1.cpp

.obj/qmake_image_collection.o: .ui/qmake_image_collection.cpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/qmake_image_collection.o .ui/qmake_image_collection.cpp

.ui/qmake_image_collection.cpp: images/filenew \
		images/fileopen \
		images/filesave \
		images/print \
		images/undo \
		images/redo \
		images/editcut \
		images/editcopy \
		images/editpaste \
		images/searchfind
	$(UIC)  -embed practica1 images/filenew images/fileopen images/filesave images/print images/undo images/redo images/editcut images/editcopy images/editpaste images/searchfind -o .ui/qmake_image_collection.cpp

